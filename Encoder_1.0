
#include <Wire.h>
int i = 0;
float t = 0;
int rpm = 0;
void setup() {
  Serial.begin(9600);
  pinMode(13, OUTPUT);
  pinMode(A3, OUTPUT);
  pinMode(A2, INPUT_PULLUP);
  Wire.begin(8);          
  Wire.onRequest(requestEvent);
}

void loop() {
  
  if(analogRead(A2) > 200){
    while(analogRead(A2) > 200){
      digitalWrite(A3, HIGH);
    }
    digitalWrite(A3, LOW);
    i++;
  }
  if ((millis()-t) > 250){
    rpm = i*120;
    Serial.println(rpm);
    i = 0;
    t = millis();
  }

}
void requestEvent() {
  Wire.write(i);
}
